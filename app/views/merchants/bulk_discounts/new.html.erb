<h1>Create New Bulk Discount</h1><br>

<%= form_with url: merchant_bulk_discounts_path(@merchant), method: :post, data: {turbo: false} do |form| %>
  <%= form.label :discount %>
  <%= form.number_field :discount %>
  <%= form.label :quantity %>
  <%= form.number_field :quantity %>
  <%= form.submit "Create Discount" %>
<% end %>